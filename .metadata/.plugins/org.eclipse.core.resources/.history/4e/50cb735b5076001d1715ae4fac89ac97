package pn;

public class Transaction {
	String date;
	double montant;
	
	public Transaction(){
		super();
	}
	

	public Transaction(String date, double montant) {
		super();
		this.date = date;
		this.montant = montant;
	}
	
	public String getDate() {
		return date;
	}
	public void setDate(String date) {
		this.date = date;
	}
	
	public double getMontant() {
		return montant;
	}
	public void setMontant(double montant) {
		this.montant = montant;
	}

	

}
