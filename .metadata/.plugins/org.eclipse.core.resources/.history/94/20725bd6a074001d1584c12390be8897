package pn;

import java.util.ArrayList;
import java.util.List;

public class CompteEpargne {
	
	private List<Double> montantsDesDébits = new ArrayList<Double>();
	private List<Double> montantsDesCredits = new ArrayList<Double>();
	private double solde;
	private double interet;
	
	public CompteEpargne(List<Double> montantsDesDébits, List<Double> montantsDesCredits, double solde,
			double interet) {
		super();
		this.montantsDesDébits = montantsDesDébits;
		this.montantsDesCredits = montantsDesCredits;
		this.solde = solde;
		this.interet = interet;
	}
	
	
	
	public List<Double> getMontantsDesDébits() {
		return montantsDesDébits;
	}


	public void setMontantsDesDébits(List<Double> montantsDesDébits) {
		this.montantsDesDébits = montantsDesDébits;
	}


	public List<Double> getMontantsDesCredits() {
		return montantsDesCredits;
	}


	public void setMontantsDesCredits(List<Double> montantsDesCredits) {
		this.montantsDesCredits = montantsDesCredits;
	}


	public double getSolde() {
		return solde;
	}


	public void setSolde(double solde) {
		this.solde = solde;
	}


	public double getInteret() {
		return interet;
	}


	public void setInteret(double interet) {
		this.interet = interet;
	}



	


}
