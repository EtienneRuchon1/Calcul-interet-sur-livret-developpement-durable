package pn;

import java.util.Date;

public class Transaction {
	Date date;
	Date dateValeur;
	double solde;
	double montant;
	double taux;
	
	public Transaction(){
		super();
	}
	
	public Transaction(Date date, Date dateValeur, double solde, double montant) {
		super();
		this.date = date;
		this.dateValeur = dateValeur;
		this.solde = solde;
		this.montant = montant;
	}
	
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	public Date getDateValeur() {
		return dateValeur;
	}
	public void setDateValeur(Date dateValeur) {
		this.dateValeur = dateValeur;
	}
	public double getSolde() {
		return solde;
	}
	public void setSolde(double solde) {
		this.solde = solde;
	}
	public double getMontant() {
		return montant;
	}
	public void setMontant(double montant) {
		this.montant = montant;
	}

	

}
