<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Règle 01 - Remplacement de la date par la date de valeur</name>
  <uuid>def5823b-d2d1-4c3a-bdc0-f913468b8bfd</uuid>
  <locale>fr_FR</locale>
  <definition><![CDATA[alors 
	affecter à 'la date de valeur de la dernière transaction' la valeur la date de la transaction precedente de 'la transaction' convertie en date ; 
	affecter à 'la date de valeur de la transaction en cours' la valeur la date de valeur de 'la transaction' précédente ;  
]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
