<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>Règle 11 - Calcul de l'intérêt après changement</name>
  <uuid>730be7af-7177-4800-bfd9-faaa5f3170b9</uuid>
  <locale>fr_FR</locale>
  <definition><![CDATA[définitions 
	soit 'le nombre de quizaines après le changement de taux' étant le nombre de quinzaines entre 'la date de changement de taux' et 'la date de valeur' ;
alors 
	assigner l'intérêt de 'le compte épargne' à ( 'le nombre de quizaines après le changement de taux' * 'le taux' * le solde de 'le compte épargne' ) / ( 100 * 24 ) ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
